import java.util.*;
class hours
{
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int x=sc.nextInt();
        int y=sc.nextInt();
        if((x*y)<12)
            System.out.println((x*y));
        else if((x*y)>24)
            System.out.println("-1");
        else
            System.out.println((x*y)-12);
    }
}
